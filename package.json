{
  "name": "openscad-tree-sitter",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for OpenSCAD with WebAssembly support for browser use",
  "main": "web-test/openscad-parser.js",
  "scripts": {
    "test": "tree-sitter test",
    "build": "tree-sitter generate",
    "build:wasm": "tree-sitter build --wasm",
    "docker:build-wasm": "docker build -t openscad-tree-sitter-wasm-builder -f Dockerfile.wasm .",
    "docker:extract-wasm": "docker create --name temp-wasm-container openscad-tree-sitter-wasm-builder && docker cp temp-wasm-container:/output/tree-sitter-openscad.wasm ./web-test/ && docker rm temp-wasm-container",
    "parse": "tree-sitter parse",
    "playground": "tree-sitter playground"
  },
  "keywords": [
    "parser",
    "tree-sitter",
    "openscad",
    "grammar",
    "syntax",
    "highlighting",
    "incremental",
    "parsing",
    "3d",
    "cad",
    "webassembly",
    "wasm",
    "browser"
  ],
  "files": [
    "grammar.js",
    "web-test/openscad-parser.js",
    "queries/*",
    "src/**"
  ],
  "author": "OpenSCAD Community",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "tree-sitter-cli": "^0.22.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/user/openscad-tree-sitter.git"
  },
  "tree-sitter": [
    {
      "scope": "source.openscad",
      "file-types": [
        "scad"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ]
    }
  ]
}
