{
  "name": "openscad-tree-sitter-monorepo",
  "version": "0.1.0",
  "description": "Monorepo for OpenSCAD Tree-sitter grammar and parser",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "env-cmd -e plain nx run-many --target=build --all",
    "build:grammar": "env-cmd -e plain nx build tree-sitter-openscad",
    "build:grammar:native": "env-cmd -e plain nx build:native tree-sitter-openscad",
    "build:grammar:wasm": "env-cmd -e plain nx build:wasm tree-sitter-openscad",
    "build:parser": "env-cmd -e plain nx build openscad-parser",
    "build:editor": "env-cmd -e plain nx build openscad-editor",
    "build:demo": "env-cmd -e plain nx build openscad-demo",
    "test": "env-cmd -e plain nx run-many --target=test --all",
    "test:grammar": "env-cmd -e plain nx test tree-sitter-openscad",
    "test:parser": "env-cmd -e plain nx test openscad-parser",
    "test:editor": "env-cmd -e plain nx test openscad-editor",
    "test:demo": "env-cmd -e plain nx test openscad-demo",
    "test:parser:file": "env-cmd -e plain nx test openscad-parser --testFile",
    "test:editor:file": "env-cmd -e plain nx test openscad-editor --testFile",
    "test:demo:file": "env-cmd -e plain nx test openscad-demo --testFile",
    "test:watch": "env-cmd -e plain nx run-many --target=test --all --watch",
    "test:coverage": "env-cmd -e plain nx run-many --target=test --all --coverage",
    "lint": "env-cmd -e plain nx run-many --target=lint --all",
    "lint:grammar": "env-cmd -e plain nx lint tree-sitter-openscad",
    "lint:parser": "env-cmd -e plain nx lint openscad-parser",
    "lint:editor": "env-cmd -e plain nx lint openscad-editor",
    "lint:demo": "env-cmd -e plain nx lint openscad-demo",
    "lint:fix": "env-cmd -e plain nx run-many --target=lint --all --fix",
    "typecheck": "env-cmd -e plain nx run-many --target=typecheck --all",
    "typecheck:parser": "env-cmd -e plain nx typecheck openscad-parser",
    "dev": "env-cmd -e plain nx run-many --target=dev --all",
    "dev:parser": "env-cmd -e plain nx dev openscad-parser",
    "dev:editor": "env-cmd -e plain nx dev openscad-editor",
    "dev:demo": "env-cmd -e plain nx dev openscad-demo",
    "serve:demo": "env-cmd -e plain nx serve openscad-demo",
    "graph": "nx graph",
    "affected": "env-cmd -e plain nx affected",
    "affected:test": "env-cmd -e plain nx affected --target=test",
    "affected:lint": "env-cmd -e plain nx affected --target=lint",
    "affected:build": "env-cmd -e plain nx affected --target=build",
    "parse": "env-cmd -e plain nx parse tree-sitter-openscad",
    "playground": "env-cmd -e plain nx playground tree-sitter-openscad",
    "clean": "nx reset",
    "reset": "nx reset && pnpm install"
  },
  "keywords": [
    "openscad",
    "tree-sitter",
    "parser",
    "grammar",
    "monorepo"
  ],
  "author": "OpenSCAD Community",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.27.0",
    "@nx/devkit": "^21.1.1",
    "@nx/eslint": "^21.1.1",
    "@nx/eslint-plugin": "^21.1.2",
    "@nx/js": "21.1.2",
    "@nx/react": "^21.1.2",
    "@nx/vite": "^21.1.1",
    "@nx/web": "21.1.1",
    "@nx/workspace": "^21.1.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/react": "16.1.0",
    "@types/node": "^22.15.18",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vitest/ui": "^3.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.1.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "^21.1.1",
    "prettier": "^2.6.2",
    "shx": "^0.3.4",
    "tslib": "^2.3.0",
    "typescript": "^5.8.3",
    "verdaccio": "^6.0.5",
    "vite": "^6.0.0",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^3.1.4",
    "vitest-fetch-mock": "^0.4.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/user/openscad-tree-sitter.git"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "nx": {
    "includedScripts": []
  }
}
