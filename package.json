{
  "name": "openscad-tree-sitter-monorepo",
  "version": "0.1.0",
  "description": "Monorepo for OpenSCAD Tree-sitter grammar and parser",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "test:grammar:parse": "cd packages/tree-sitter-openscad && npx tree-sitter parse",
    "test:grammar:real-world": "cd packages/tree-sitter-openscad && node test-real-world-cli.js",

    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "setup:local-registry": "pnpm verdaccio &",
    "publish:local": "pnpm run setup:local-registry && pnpm publish --registry http://localhost:4873 && pnpm unpublish --registry http://localhost:4873 --force && pnpm run teardown:local-registry",
    "teardown:local-registry": "kill-port 4873",
    "clean": "pnpm run clean:node-modules && pnpm run clean:dist",
    "clean:node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "clean:dist": "find . -name 'dist' -type d -prune -exec rm -rf '{}' +",
    "reinstall": "pnpm run clean && pnpm install"
  },
  "keywords": [
    "openscad",
    "tree-sitter",
    "parser",
    "grammar",
    "monorepo"
  ],
  "author": "OpenSCAD Community",
  "license": "MIT",
  "packageManager": "pnpm@10.10.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.27.0",
    "@nx/devkit": "21.1.3",
    "@nx/eslint": "^21.1.1",
    "@nx/eslint-plugin": "^21.1.2",
    "@nx/js": "21.1.2",
    "@nx/playwright": "21.1.3",
    "@nx/react": "^21.1.2",
    "@nx/vite": "^21.1.1",
    "@nx/web": "21.1.1",
    "@nx/workspace": "^21.1.1",
    "@playwright/test": "^1.52.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/react": "16.1.0",
    "@types/node": "^22.15.18",
    "@types/react": "19.0.0",
    "@types/react-dom": "19.0.0",
    "@types/resolve": "1.20.6",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vitest/ui": "^3.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "find-up": "7.0.0",
    "globals": "^16.1.0",
    "jiti": "2.4.2",
    "jsdom": "~22.1.0",
    "nx": "^21.1.1",
    "playwright": "1.52.0",
    "prettier": "^2.6.2",
    "resolve": "1.22.10",
    "shx": "^0.3.4",
    "tslib": "^2.3.0",
    "typescript": "^5.8.3",
    "verdaccio": "^6.0.5",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.1.4",
    "vitest-fetch-mock": "^0.4.5"
  },
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/holistic-stack/openscad-tree-sitter.git"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "tree-sitter": "0.22.4"
  },
  "nx": {
    "includedScripts": []
  }
}
