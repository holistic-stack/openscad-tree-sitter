{
  "name": "openscad-parser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/openscad-parser/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/openscad-parser && vite build"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/openscad-parser && vite build --watch"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/openscad-parser && vitest run"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "self"
        }
      ]
    },
    "test:watch": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/openscad-parser && vitest"
      }
    },
    "test:coverage": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/openscad-parser && vitest run --coverage"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/openscad-parser && eslint ."
      }
    },
    "lint:fix": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/openscad-parser && eslint . --fix"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "cd packages/openscad-parser && tsc --noEmit"
      }
    }
  },
  "tags": []
}
