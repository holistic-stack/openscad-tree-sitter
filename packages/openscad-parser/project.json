{
  "name": "openscad-parser",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/openscad-parser/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/openscad-parser",
        "configFile": "packages/openscad-parser/vite.config.ts"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies"
        }
      ]
    },
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "openscad-parser:build:development",
        "configFile": "packages/openscad-parser/vite.config.ts"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/packages/openscad-parser",
        "configFile": "packages/openscad-parser/vite.config.ts"
      },
      "configurations": {
        "watch": {
          "watch": true
        },
        "coverage": {
          "coverage": true
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/openscad-parser/**/*.ts"]
      }
    },
    "lint:fix": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["packages/openscad-parser/**/*.ts"],
        "fix": true
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --noEmit --project packages/openscad-parser/tsconfig.json",
        "cwd": "."
      }
    }
  },
  "tags": ["type:library", "scope:parser", "platform:node"]
}
