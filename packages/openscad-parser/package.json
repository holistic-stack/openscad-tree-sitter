{
  "name": "@holistic-stack/openscad-parser",
  "version": "0.1.0",
  "description": "TypeScript parser for OpenSCAD using tree-sitter",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "tsc --noEmit && eslint . && prettier --check .",
    "prepublishOnly": "pnpm build",
    "docs:ast": "typedoc --options typedoc.json"
  },
  "keywords": [
    "openscad",
    "parser",
    "ast",
    "tree-sitter",
    "typescript"
  ],
  "author": "OpenSCAD Community",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "dependencies": {
    "@openscad/tree-sitter-openscad": "workspace:*",
    "find-up": "7.0.0",
    "resolve": "1.22.10",
    "web-tree-sitter": "0.25.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@nx/vite": "^21.1.1",
    "@types/node": "^22.15.18",
    "@types/resolve": "1.20.6",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/eslint-plugin": "^1.2.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "^2.8.8",
    "ts-node": "10.9.2",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.6.4",
    "typescript": "^5.8.3",
    "vite": "^6.0.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.1.4",
    "vitest-fetch-mock": "^0.4.5"
  }
}
