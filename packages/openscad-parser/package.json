{
  "name": "@openscad/parser",
  "version": "0.1.0",
  "description": "TypeScript parser for OpenSCAD using tree-sitter",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "env-cmd -e plain vite build",
    "dev": "vite build --watch",
    "test": "env-cmd -e plain vitest run",
    "test:watch": "vitest",
    "test:coverage": "env-cmd -e plain vitest run --coverage",
    "lint": "env-cmd -e plain eslint .",
    "lint:fix": "env-cmd -e plain eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "env-cmd -e plain tsc --noEmit",
    "check": "pnpm typecheck && pnpm lint && pnpm format:check"
  },
  "keywords": [
    "openscad",
    "parser",
    "ast",
    "tree-sitter",
    "typescript"
  ],
  "author": "OpenSCAD Community",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "dependencies": {
    "@openscad/tree-sitter-openscad": "workspace:*",
    "web-tree-sitter": "0.25.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/node": "^22.15.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/eslint-plugin": "^1.2.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.1.0",
    "jsdom": "^26.1.0",
    "node-fetch": "^3.3.2",
    "prettier": "^2.8.8",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^3.1.4",
    "vitest-fetch-mock": "^0.4.5",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
